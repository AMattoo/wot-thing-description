@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix td: <http://www.w3.org/ns/td#> .

[] a td:DataSchema ;
   rdfs:comment "Humidity as a percentage" ;
   td:property (
     [
       td:name "humidity" ;
       td:value [
         td:type td:number ;
         xsd:minInclusive "0" ;
         xsd:maxInclusive "100" ;
       ]
     ]
   ) .