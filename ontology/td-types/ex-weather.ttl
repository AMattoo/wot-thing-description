@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix td: <http://www.w3.org/ns/td#> .

[] a td:DataSchema ;
   rdfs:comment "Weather station" ;
   td:property (
     [
       td:name "wind" ;
       td:property (
         [ td:name "speed" ;
           td:value [
             td:type td:number
           ]
         ]
         [ td:name "direction" ;
           td:value [
             td:type td:integer
           ]
         ]
       )
     ]
     [ td:name "temperature" ;
       td:value [
         td:type td:number
       ]
     ]
     [ td:name "humidity" ;
       td:value [
         td:type td:integer
       ]
     ]
   ) .