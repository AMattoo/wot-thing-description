@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix td: <http://www.w3.org/ns/td#> .

[
  a td:DataSchema ;
  rdfs:comment "Generic sensor reading" ;
    td:property (
        [
          td:name "reading" ;
          td:value [
            td:anyOf (td:number td:string)
          ]
        ]
    ) .
]