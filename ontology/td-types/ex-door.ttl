@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix td: <http://www.w3.org/ns/td#> .

[] a td:DataSchema ;
   rdfs:comment "Whether a door is open or closed" ;
   td:property (
     [
       td:name "door" ;
       td:value [
         td:enum ("Open" "Closed")
       ]
     ]
   ) .
  